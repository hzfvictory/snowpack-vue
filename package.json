{
  "scripts": {
    "start": "snowpack dev",
    "buildTest": "cross-env NODE_ENV=buildTest snowpack build",
    "build": "snowpack build",
    "prettier": "node ./scripts/prettier.js",
    "lint-staged": "lint-staged",
    "pretty-quick": "pretty-quick --staged --ignore-path=.prettierignore"
  },
  "dependencies": {
    "vue": "3.0.0"
