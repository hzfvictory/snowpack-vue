    "@snowpack/plugin-webpack": "^2.3.1",
    "autoprefixer": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.16.0",
    "http2-proxy": "^5.0.53",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "postcss-cli": "^8.3.1",
    "postcss-scss": "^3.0.4",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "sass": "^1.30.0",
    "snowpack": "^3.4.0"
